#include<stdio.h>
int main()
{
	int x,n,a[1000],i,j;//initialize array to zero
	char s;
	scanf("%d %d",&x,&n);
	for(i=0;i<n*9;i++)
		a[i]=0;
	for(i=0;i<n;i++)
		for(j=0;j<54;j++)
		{
			if(j<37)
			{
				scanf("%c",&s);
				if(s=='0')
					a[(j/4)+i]++;
			}
			/*else
			{
				scanf("%c",&s);
				if(s=='0')
					a[((54-j)/2)+i]++;
			}*/
		}
	for(i=0;i<n*9;i++)
		printf("%d\n",a[i]);
	return 0;
}




	

